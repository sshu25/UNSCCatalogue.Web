@model List<Category>
@{
    ViewBag.Title = Strings.Categories;
}

<table class="table">
    <tr>
        <th>@Strings.ID</th>
        <th>@Strings.Name</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
        </tr>
    }
</table>